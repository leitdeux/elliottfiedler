{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "description": "Personal website of Elliott Fiedler.",
  "author": "Elliott Fiedler <elliott@elliottfiedler.com>",
  "scripts": {
    "dev": "gatsby develop -p 9999",
    "dev:clean": "npm run clean && npm run dev",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "clean:all": "gatsby clean && rm -rf ./node_modules && npm i",
    "deploy:staging": "npm run clean && npm run -n \"-r dotenv/config\" && GATSBY_ACTIVE_ENV=staging npm run build && NETLIFY_DEPLOY",
    "deploy:production": "npm run clean && npm run -n \"-r dotenv/config\" && npm run build && NETLIFY_DEPLOY"
  },
  "dependencies": {
    "@csstools/normalize.css": "^11.0.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@theme-ui/preset-system": "^0.13.0",
    "dotenv": "^8.6.0",
    "gatsby": "^3.4.0",
    "gatsby-plugin-google-analytics": "^3.4.0",
    "gatsby-plugin-google-tagmanager": "^3.6.0",
    "gatsby-plugin-manifest": "^3.4.0",
    "gatsby-plugin-mdx": "^2.14.0",
    "gatsby-plugin-react-helmet": "^4.5.0",
    "gatsby-plugin-react-intl": "^3.0.2",
    "gatsby-plugin-robots-txt": "^1.6.14",
    "gatsby-plugin-sitemap": "^4.0.0",
    "gatsby-plugin-theme-ui": "^0.12.0",
    "gatsby-source-filesystem": "^3.4.0",
    "luxon": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "theme-ui": "^0.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^2.4.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-jsx-a11y": "^6.5.1"
  }
}
